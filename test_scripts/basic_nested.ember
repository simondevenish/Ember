// Simple nested property test
print("==== Basic Nested Property Test ====");

// Create a simple object
var obj = {
    x: {
        y: "original"
    }
};

// Print initial value
print("Initial value: " + obj.x.y);

// Get the inner object
var inner = obj.x;

// Update the property
inner.y = "updated";

// Set it back to the parent object
obj.x = inner;

// Verify the update
print("Updated value: " + obj.x.y); 