// Complex test for multi-level nested properties

print("=== Testing Multi-Level Nested Properties ===");

// Create object
var data = {
    user: {
        profile: {
            name: "Initial Name"
        }
    }
};

// Print initial structure
print("Initial data.user.profile.name = " + data.user.profile.name);

// Modify nested property
data.user.profile.name = "Updated Name";
print("After first update: data.user.profile.name = " + data.user.profile.name);

// Add a new deeply nested property path
data.user.profile.address = {
    city: "New York",
    zipcode: "10001"
};

// Access the new nested properties
print("data.user.profile.address.city = " + data.user.profile.address.city);
print("data.user.profile.address.zipcode = " + data.user.profile.address.zipcode);

// Create property chain that doesn't exist yet
data.settings = {};
data.settings.theme = {};
data.settings.theme.colors = {
    background: "#FFFFFF",
    text: "#000000"
};

print("data.settings.theme.colors.background = " + data.settings.theme.colors.background);
print("data.settings.theme.colors.text = " + data.settings.theme.colors.text);

print("=== Multi-Level Test Complete ==="); 