// Simple nested property test for debugging
var obj = { x: { y: "original" } };

// Print initial value
print("Initial x.y: " + obj.x.y);

// Get a reference to the inner object for comparison
var inner = obj.x;
print("Direct inner.y: " + inner.y);

// Update nested property
obj.x.y = "updated";

// Print updated value
print("Updated obj.x.y: " + obj.x.y);
print("Updated inner.y: " + inner.y); 