// Simple multi-level test

print("=== Testing Multi-Level Nested Properties ===");

// Create object
var obj = {};

// Add first level
obj.level1 = {};
print("Added obj.level1");

// Add second level
obj.level1.level2 = {};
print("Added obj.level1.level2");

// Add value at third level
obj.level1.level2.value = "Success!";
print("Set obj.level1.level2.value = 'Success!'");

// Read the nested value back
print("Reading: obj.level1.level2.value = " + obj.level1.level2.value);

print("=== Test Complete ==="); 