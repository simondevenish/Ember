// Define functions
function add(a, b) {
    return a + b;
}

function subtract(a, b) {
    return a - b;
}

// Create object with methods
var calculator = {
    name: "Simple Calculator",
    description: "A calculator object with basic operations"
};

// Assign functions to object properties
calculator.add = add;
calculator.subtract = subtract;

// Call methods
print("Calculator: " + calculator.name);
print("5 + 3 = " + calculator.add(5, 3));
print("10 - 4 = " + calculator.subtract(10, 4)); 