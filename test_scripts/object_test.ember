// Object Test Script
// This script tests object literals, property access, property assignment, and method calls

// Create a simple object with properties
var player = {
    name: "Hero",
    health: 100,
    mana: 50,
    attack: 15,
    defense: 10,
    // Method to calculate damage
    dealDamage: function(target) {
        return this.attack - target.defense;
    }
};

// Create an enemy object
var enemy = {
    name: "Goblin",
    health: 50,
    attack: 8,
    defense: 5
};

// Test property access
print("Initial player name: " + player.name);

// Test property assignment
player.name = "Paladin";
print("New player name: " + player.name);

// Add a new property dynamically
player.level = 5;
print("Player level: " + player.level);

// Test object properties
print("Player health: " + player.health);
print("Enemy attack: " + enemy.attack);

// Nested property access
var game = {
    settings: {
        difficulty: "Normal",
        sound: true,
        graphics: "High"
    }
};

// Test nested property access
print("Game difficulty: " + game.settings.difficulty);

// Update nested property
game.settings.difficulty = "Hard";
print("New game difficulty: " + game.settings.difficulty);

// Test method call
var damage = player.dealDamage(enemy);
print("Player deals " + damage + " damage to enemy");

// Create an object with multiple methods
var calculator = {
    add: function(a, b) {
        return a + b;
    },
    subtract: function(a, b) {
        return a - b;
    },
    multiply: function(a, b) {
        return a * b;
    }
};

// Test multiple method calls
print("Addition: " + calculator.add(5, 3));
print("Subtraction: " + calculator.subtract(10, 4));
print("Multiplication: " + calculator.multiply(6, 7)); 