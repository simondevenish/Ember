// Simple Object Test
// Testing basic object properties without methods

// Create a simple object
var person = {
    name: "John",
    age: 30,
    city: "New York"
};

// Access properties
print("Person: " + person.name + ", " + person.age + ", " + person.city);

// Modify a property
person.age = 31;
print("Updated age: " + person.age);

// Nested object
var company = {
    name: "TechCorp",
    location: {
        city: "San Francisco",
        state: "CA"
    }
};

// Access nested property
print("Company location: " + company.location.city + ", " + company.location.state); 