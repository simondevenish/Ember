// Simple property test
var obj = {};
print("Created empty object");

// Set a property
obj.x = "test";
print("Set obj.x = test");

// Get the property
var val = obj.x;
print("obj.x = " + val);

// Modify the property
obj.x = "modified";
print("Modified obj.x = modified");

// Get the property again
val = obj.x;
print("obj.x = " + val); 